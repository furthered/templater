{
    "name": "furthered/templater",
    "description": "Template helper, extends Blade, registers various formatters",
    "authors": [
        {
            "name": "Joe Tannenbaum",
            "role": "Author"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/furthered/assets.git"
        }
    ],
    "require": {
        "illuminate/support": "5.4.*",
        "illuminate/view": "5.4.*",
        "nesbot/carbon": "^1.20",
        "furthered/assets": "0.3.*"
    },
    "autoload": {
        "psr-4": {
            "Templater\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Templater\\Test\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4"
    }
}
